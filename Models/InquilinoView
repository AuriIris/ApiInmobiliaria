using api.Models;

    public class InquilinoView
    {
        public int Id { get; set; }
        public string? Dni { get; set; }="";
        public string? Apellido { get; set; }="";
        public string? Nombre { get; set; }="";
        public string? Telefono { get; set; }="";
        public string? Mail { get; set; }="";

        public InquilinoView()
        {
        }

        public InquilinoView(Inquilino inquilino)
        {
            this.Id = inquilino.Id;
            this.Dni = inquilino.Dni;
            this.Apellido = inquilino.Apellido;
            this.Nombre = inquilino.Nombre;
            this.Telefono = inquilino.Telefono;
            this.Mail = inquilino.Mail;
        }
    }

